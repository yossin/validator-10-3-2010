<UserControl x:Class="The_Validator11.ColoredObjectRow_AddNew"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="21" Width="Auto">
    <Grid Height="Auto">
        <WrapPanel Margin="0,0" Background="White" >
            <Button Name="ColoredObjectRowAddNew" FontSize="14" Height="12" Width="11" Padding="-4,-8" Margin="0,2,0,0">
                <Image Source="img\plus.jpg"></Image>
            </Button>
            <TextBox Name="ColoredObjectRow_NewValueTextBox" Padding="1,4" Height="21" Width="119" Text="New" BorderThickness="0" LostFocus="TextFocusLost">            </TextBox>
        </WrapPanel>
    </Grid>
</UserControl>
