<UserControl x:Class="The_Validator11.ConvertionRow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="26" Width="603">
    <Grid Height="26" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        
        <TextBox Name="ConvertionGridRowFrom" Grid.Row="0" Grid.Column="0" Height="22" Width="Auto" Text="from">        </TextBox>
        <TextBox Name="ConvertionGridRowTo"   Grid.Row="1" Grid.Column="1" Height="22" Width="Auto" Text="to">          </TextBox>
        
    </Grid>
</UserControl>
